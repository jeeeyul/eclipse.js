<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="net.jeeeyul.eclipsejs.ui.QueryView"
            icon="icons/eclipse-js.gif"
            id="net.jeeeyul.eclipsejs.ui.QueryView"
            name="Eclipse.JS"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="net.jeeeyul.eclipsejs.ui.CommonView"
            id="net.jeeeyul.eclipsejs.common.view"
            name="Eclipse.JS Views"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.wst.jsdt.core.JsGlobalScopeContainerInitializer">
         <JsGlobalScopeContainerInitializer
            class="net.jeeeyul.eclipsejs.core.EJSLibraryInitializer"
            id="net.jeeeyul.eclipsejs.lib">
      </JsGlobalScopeContainerInitializer>
   </extension>
   <extension
         point="org.eclipse.wst.jsdt.ui.JsGlobalScopeContainerPage">
         <JsGlobalScopeContainerPage
         	id="net.jeeeyul.eclipsejs.lib"
         	name="Eclipse.JS Libraries"
         	class="net.jeeeyul.eclipsejs.core.EJSLibPage"
         	>
         </JsGlobalScopeContainerPage>
   </extension>
   
      <extension point="org.eclipse.wst.jsdt.ui.JsGlobalScopeUIInitializer">
     <JsGlobalScopeUIInitializer
     	id="net.jeeeyul.eclipsejs.lib"
     	class="net.jeeeyul.eclipsejs.core.EJSLibUiInitializer">
     </JsGlobalScopeUIInitializer>
   </extension>
      <extension
            point="org.eclipse.ui.console.consoleFactories">
         <consoleFactory
               class="net.jeeeyul.eclipsejs.EJSConsoleFactory"
               icon="icons/eclipse-js.gif"
               label="Eclipse.JS">
         </consoleFactory>
      </extension>
      <extension
            point="org.eclipse.ui.menus">
         <menuContribution
               allPopups="false"
               locationURI="menu:showView">
            <dynamic
                  class="net.jeeeyul.eclipsejs.ui.ShowViewContribution"
                  id="net.jeeeyul.eclipsejs.dynamic1">
            </dynamic>
         </menuContribution>
         <menuContribution
               allPopups="false"
               locationURI="toolbar:net.jeeeyul.eclipsejs.ui.QueryView">
            <command
                  commandId="net.jeeeyul.eclipsejs.execute.view.script"
                  icon="icons/run_exc.gif"
                  style="push">
            </command>
         </menuContribution>
      </extension>
      <extension
            point="org.eclipse.ui.commands">
         <command
               id="net.jeeeyul.eclipsejs.common.command"
               name="Eclipse.JS Command">
            <commandParameter
                  id="net.jeeeyul.eclipsejs.command.id"
                  name="E.JS Command ID"
                  optional="true">
            </commandParameter>
         </command>
         <command
               defaultHandler="net.jeeeyul.eclipsejs.ui.ExecuteViewScriptHandler"
               id="net.jeeeyul.eclipsejs.execute.view.script"
               name="Excute View Script">
         </command>
      </extension>
      <extension
            point="org.eclipse.ui.bindings">
         <key
               commandId="net.jeeeyul.eclipsejs.execute.view.script"
               contextId="net.jeeeyul.eclipsejs.script.view"
               schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
               sequence="M1+ENTER">
         </key>
      </extension>
      <extension
            point="org.eclipse.ui.contexts">
         <context
               id="net.jeeeyul.eclipsejs.script.view"
               name="In Eclipse.JS Script View">
         </context>
      </extension>
      <extension
            point="org.eclipse.ui.editors.templates">
         <template
               autoinsert="false"
               contextTypeId="javaScript"
               description="Add a listener to SWT Widget"
               icon="icons/eclipse-js.gif"
               id="net.jeeeyul.eclipsejs.template.swt.listener"
               name="addListener">
            <pattern>
               ${shell:localVar}.addListener(SWT.${eventType}, {
	handleEvent : function(/*org.eclipse.swt.widgets.Event*/ e){
		${cursor}
	}
});
            </pattern>
         </template>
         <template
               autoinsert="false"
               contextTypeId="javaScript"
               description="Creates View Extension Module"
               icon="icons/eclipse-js.gif"
               id="net.jeeeyul.eclipsejs.template.extension.view"
               name="View">
            <pattern>
               module = View.extend({
	&quot;name&quot; : &quot;${viewName}&quot;,
	&quot;icon&quot; : &quot;${icon}&quot;,
	&quot;init&quot; : function(viewSite){
		this.viewSite = viewSite;
	},
	&quot;create&quot; : function(/*org.eclipse.swt.widgets.Shell*/ parent) {
		${cursor}
	},
	&quot;dispose&quot; : function() {
	}
});
            </pattern>
         </template>
      </extension>
      <extension
            point="org.eclipse.wst.jsdt.core.inferrenceSupport">
            <inferenceProvider
            	class="net.jeeeyul.eclipsejs.jsdt.AdvancedInferenceProvider"/>
      </extension>

</plugin>
